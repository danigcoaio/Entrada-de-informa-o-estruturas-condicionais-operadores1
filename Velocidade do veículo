#include <iostream>
using namespace std;

int main()
{
    float velocidade; 

    cout << "Digite a velocidade: " << endl; 
    cin >> velocidade; 
    
    if(velocidade <= 80) { 
        cout << "Velocidade segura: " << endl; 
} 
    else { 
        cout << "Velocidade perigosa: " << endl; 
}
   return 0;
} 

