#include <iostream>
using namespace std;

int main()
{
    float numero1; 
    float numero2;

    cout << "Digite o primeiro número: " << endl; 
    cin >> numero1; 
    
    cout << "Digite o segundo número: " << endl; 
    cin >> numero2; 
    
    if(numero1 == numero2) { 
        cout << "Os números são iguais: " << endl; 
} 
    else if(numero1 > numero2) { 
        cout << "O primeiro número é maior: " << endl; 
} 
    else { 
        cout << "O segundo número é maior:" << endl;
}
   
   return 0;
}
